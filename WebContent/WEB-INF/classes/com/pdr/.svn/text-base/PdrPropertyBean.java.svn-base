package com.pdr;
import java.util.HashMap;

public class PdrPropertyBean {
	private String outputType;
	private HashMap sharePointFolder;
	private String[] excludeDir;
	private String[] excludeFile;
	private String[] includeDir;
	private String[] includeFile;
	
	
	public HashMap getSharePointFolder() {
		return sharePointFolder;
	}
	public void setSharePointFolder(HashMap sharePointFolder) {
		this.sharePointFolder = sharePointFolder;
	}
	
	
	public String getOutputType() {
		return outputType;
	}
	public void setOutputType(String outputType) {
		this.outputType = outputType;
	}
	public String[] getExcludeDir() {
		return excludeDir;
	}
	public void setExcludeDir(String[] excludeDir) {
		this.excludeDir = excludeDir;
	}
	public String[] getExcludeFile() {
		return excludeFile;
	}
	public void setExcludeFile(String[] excludeFile) {
		this.excludeFile = excludeFile;
	}
	public String[] getIncludeDir() {
		return includeDir;
	}
	public void setIncludeDir(String[] includeDir) {
		this.includeDir = includeDir;
	}
	public String[] getIncludeFile() {
		return includeFile;
	}
	public void setIncludeFile(String[] includeFile) {
		this.includeFile = includeFile;
	}
}
